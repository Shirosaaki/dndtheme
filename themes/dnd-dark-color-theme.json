{
  "$schema": "vscode://schemas/color-theme",
  "name": "D&D Dark",
  "type": "dark",
  "colors": {
    // Editor Colors - Custom Palette Theme
    "editor.background": "#2F2F2F",
    "editor.foreground": "#FFFFFF",
    "editorCursor.foreground": "#FFD700",
    "editor.lineHighlightBackground": "#3F3F3F",
    "editor.selectionBackground": "#FF450040",
    "editor.selectionHighlightBackground": "#FF634720",
    "editor.findMatchBackground": "#FFD70060",
    "editor.findMatchHighlightBackground": "#FFD70030",
    "editor.wordHighlightBackground": "#FF450020",
    "editor.wordHighlightStrongBackground": "#FF634730",
    
    // Activity Bar - Orange Red Theme
    "activityBar.background": "#A65E2E",
    "activityBar.foreground": "#FFFFFF",
    "activityBar.inactiveForeground": "#FFFFFF80",
    "activityBarBadge.background": "#FFD700",
    "activityBarBadge.foreground": "#2F2F2F",
    
    // Side Bar - Dark with Gold Accents
    "sideBar.background": "#2F2F2F",
    "sideBar.foreground": "#FFFFFF",
    "sideBarTitle.foreground": "#FFD700",
    "sideBarSectionHeader.background": "#FF6347",
    "sideBarSectionHeader.foreground": "#FFFFFF",
    
    // Explorer
    "tree.indentGuidesStroke": "#FF634750",
    "list.activeSelectionBackground": "#FF6347",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.inactiveSelectionBackground": "#FF450080",
    "list.hoverBackground": "#FF634730",
    "list.focusBackground": "#FF6347",
    
    // Status Bar - Tomato Theme
    "statusBar.background": "#E8CBAE",
    "statusBar.foreground": "#4C4C4C",
    "statusBar.debuggingBackground": "#FF4500",
    "statusBar.noFolderBackground": "#2F2F2F",
    
    // Tab Bar - Custom Colors
    "tab.activeBackground": "#2F2F2F",
    "tab.activeForeground": "#FFFFFF",
    "tab.inactiveBackground": "#1F1F1F",
    "tab.inactiveForeground": "#FFFFFF80",
    "tab.border": "#FF634750",
    "tab.activeBorder": "#FFD700",
    "tab.unfocusedActiveBorder": "#FF6347",
    
    // Editor Group & Tabs
    "editorGroupHeader.tabsBackground": "#2F2F2F",
    "editorGroupHeader.tabsBorder": "#FF634750",
    
    // Terminal - Custom Color Scheme
    "terminal.background": "#2F2F2F",
    "terminal.foreground": "#FFFFFF",
    "terminal.ansiBlack": "#2F2F2F",
    "terminal.ansiRed": "#FF4500",
    "terminal.ansiGreen": "#FFD700",
    "terminal.ansiYellow": "#FFD700",
    "terminal.ansiBlue": "#FF6347",
    "terminal.ansiMagenta": "#FF4500",
    "terminal.ansiCyan": "#FFD700",
    "terminal.ansiWhite": "#FFFFFF",
    "terminal.ansiBrightBlack": "#4F4F4F",
    "terminal.ansiBrightRed": "#FF6347",
    "terminal.ansiBrightGreen": "#FFD700",
    "terminal.ansiBrightYellow": "#FFD700",
    "terminal.ansiBrightBlue": "#FF6347",
    "terminal.ansiBrightMagenta": "#FF4500",
    "terminal.ansiBrightCyan": "#FFD700",
    "terminal.ansiBrightWhite": "#FFFFFF",
    
    // Panel (Terminal, Output, etc) - Custom Colors
    "panel.background": "#2F2F2F",
    "panel.border": "#FF634750",
    "panelTitle.activeForeground": "#FFD700",
    "panelTitle.inactiveForeground": "#FFFFFF80",
    
    // Input Controls - Custom Colors
    "input.background": "#2F2F2F",
    "input.foreground": "#FFFFFF",
    "input.border": "#FF6347",
    "input.placeholderForeground": "#FFFFFF60",
    "inputOption.activeBorder": "#FFD700",
    
    // Dropdown - Custom Colors
    "dropdown.background": "#2F2F2F",
    "dropdown.foreground": "#FFFFFF",
    "dropdown.border": "#FF6347",
    
    // Button - Custom Colors
    "button.background": "#FF4500",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#FF6347",
    
    // Notification - Custom Colors
    "notificationCenter.border": "#FF6347",
    "notificationCenterHeader.background": "#2F2F2F",
    "notificationToast.border": "#FF6347",
    
    // Extensions - Custom Colors
    "extensionButton.prominentBackground": "#FF4500",
    "extensionButton.prominentForeground": "#FFFFFF",
    "extensionButton.prominentHoverBackground": "#FF6347",
    "notifications.background": "#2a241d",
    "notifications.foreground": "#e8dcc0",
    "notifications.border": "#4a3d2a",
    
    // Title Bar
    "titleBar.activeBackground": "#1a1611",
    "titleBar.activeForeground": "#c9a96e",
    "titleBar.inactiveBackground": "#0f0e0d",
    "titleBar.inactiveForeground": "#6b5b73",
    
    // Menu Bar
    "menubar.selectionBackground": "#2a241d",
    "menubar.selectionForeground": "#e8dcc0",
    "menu.background": "#2a241d",
    "menu.foreground": "#e8dcc0",
    "menu.selectionBackground": "#3d4f2f",
    "menu.selectionForeground": "#e8dcc0",
    
    // Scrollbar
    "scrollbar.shadow": "#00000050",
    "scrollbarSlider.background": "#4a3d2a80",
    "scrollbarSlider.activeBackground": "#6b5b73",
    "scrollbarSlider.hoverBackground": "#4a3d2a",
    
    // Badge
    "badge.background": "#8b4513",
    "badge.foreground": "#ffffff",
    
    // Progress Bar
    "progressBar.background": "#d4af37",
    
    // Minimap
    "minimap.background": "#1a1611",
    "minimapSlider.background": "#4a3d2a80",
    "minimapSlider.hoverBackground": "#4a3d2a",
    "minimapSlider.activeBackground": "#6b5b73",
    
    // Editor Widget
    "editorWidget.background": "#2a241d",
    "editorWidget.border": "#4a3d2a",
    "editorSuggestWidget.background": "#2a241d",
    "editorSuggestWidget.border": "#4a3d2a",
    "editorSuggestWidget.foreground": "#e8dcc0",
    "editorSuggestWidget.selectedBackground": "#3d4f2f",
    
    // Peek View
    "peekView.border": "#d4af37",
    "peekViewEditor.background": "#2a241d",
    "peekViewResult.background": "#1a1611",
    "peekViewTitle.background": "#4a3d2a",
    
    // Git Colors
    "gitDecoration.modifiedResourceForeground": "#d4af37",
    "gitDecoration.deletedResourceForeground": "#cc6666",
    "gitDecoration.untrackedResourceForeground": "#6aa04a",
    "gitDecoration.ignoredResourceForeground": "#6b5b73",
    "gitDecoration.conflictingResourceForeground": "#b294bb"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6b5b73"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#e8dcc0"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#d4af37"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#b294bb"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#8b7d6b"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#5f819d"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#d4af37"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#6aa04a"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#d4af37"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type"
      ],
      "settings": {
        "foreground": "#5fb3b3"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#5fb3b3"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cc6666"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": [
        "entity.name.method.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5f819d"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#5f819d"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#b294bb"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#d4af37"
      }
    },
    {
      "name": "CSS Classes",
      "scope": [
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#d4af37"
      }
    },
    {
      "name": "CSS ID's",
      "scope": [
        "source.sass keyword.control"
      ],
      "settings": {
        "foreground": "#5f819d"
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#6aa04a"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#b294bb"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#5fb3b3"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#5fb3b3"
      }
    },
    {
      "name": "URL",
      "scope": [
        "*url*",
        "*link*",
        "*uri*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5f819d"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cc6666"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#b294bb"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#d4af37"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#6aa04a"
      }
    },
    {
      "name": "JSON Key - Level 3",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#cc6666"
      }
    },
    {
      "name": "JSON Key - Level 4",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#5f819d"
      }
    },
    {
      "name": "JSON Key - Level 5",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#5fb3b3"
      }
    },
    {
      "name": "JSON Key - Level 6",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#b294bb"
      }
    },
    {
      "name": "JSON Key - Level 7",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#d4af37"
      }
    },
    {
      "name": "JSON Key - Level 8",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#6aa04a"
      }
    },
    {
      "name": "Markdown - Plain",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#e8dcc0"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown"
      ],
      "settings": {
        "foreground": "#b294bb"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline Punctuation",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
      ],
      "settings": {
        "foreground": "#8b7d6b"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#d4af37",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b294bb"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": [
        "markup.bold",
        "markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#b294bb"
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#b294bb"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#6aa04a"
      }
    },
    {
      "name": "Markup - Strike",
      "scope": [
        "markup.strike"
      ],
      "settings": {
        "fontStyle": "strikethrough",
        "foreground": "#cc6666"
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": [
        "markup.quote punctuation.definition.blockquote.markdown"
      ],
      "settings": {
        "foreground": "#8b7d6b"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "string.other.link.title.markdown"
      ],
      "settings": {
        "foreground": "#5f819d"
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": [
        "string.other.link.description.title.markdown"
      ],
      "settings": {
        "foreground": "#b294bb"
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": [
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "#d4af37"
      }
    },
    {
      "name": "Markup - Raw Block",
      "scope": [
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#b294bb"
      }
    },
    {
      "name": "Markdown - Raw Block Fenced",
      "scope": [
        "markup.raw.block.fenced.markdown"
      ],
      "settings": {
        "foreground": "#e8dcc020"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block",
      "scope": [
        "punctuation.definition.fenced.markdown"
      ],
      "settings": {
        "foreground": "#e8dcc020"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block Variable",
      "scope": [
        "markup.raw.block.fenced.markdown",
        "variable.language.fenced.markdown",
        "punctuation.section.class.end"
      ],
      "settings": {
        "foreground": "#e8dcc0"
      }
    },
    {
      "name": "Markdown - Fenced Language",
      "scope": [
        "variable.language.fenced.markdown"
      ],
      "settings": {
        "foreground": "#8b7d6b"
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": [
        "meta.separator"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#8b7d6b"
      }
    },
    {
      "name": "Markup - Table",
      "scope": [
        "markup.table"
      ],
      "settings": {
        "foreground": "#e8dcc0"
      }
    }
  ]
}